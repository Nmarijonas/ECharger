@model IEnumerable<EChargerInternational.EChargerInternational.Remejai>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Pavadinimas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SkirtosLesos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aprasymas)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pavadinimas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkirtosLesos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aprasymas)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdRemėjai">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdRemėjai">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdRemėjai">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
